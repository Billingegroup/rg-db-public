            "notes": { "required": false, "anyof_type": ["string", "list"] },
            "year": { "required": false, "type": "integer" },
            "begin_year": { "required": false, "type": "integer" },
            "begin_day": { "required": false, "type": "integer" },
            "begin_month": {
              "description": "Use month and year if the service",
              "required": false,
