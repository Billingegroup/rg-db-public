            "other": { "required": false, "anyof_type": ["string", "list"] },
            "type": {
              "required": true,
              "type": "string",
              "description": "profession, department, school, university",
              "eallowed": ["profession", "university", "school", "department"]
            }
          }
        },
        "type": "list"
      },
      "skills": {
        "description": "Skill the group member has",
